{
  "preset": "node-style-guide",
  "disallowMultipleVarDecl": true,
  "requireTrailingComma": null,
  "requireCapitalizedComments": null,
  "requireSpaceAfterKeywords": [
    "if",
    "else",
    "for",
    "while",
    "do",
    "switch",
    "case",
    "return",
    "try",
    "typeof"
  ],
  "excludeFiles": ["node_modules/**", "bower_modules/**", "server/swagger-ui/**"]   
}